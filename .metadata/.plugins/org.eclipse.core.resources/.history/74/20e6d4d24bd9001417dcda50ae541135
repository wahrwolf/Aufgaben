import java.util.ArrayList;


public class Term {
	
	private double _koeffizient;
	private String _variable;

	public Term()
	{
		
	}
	
	public double getKoeffizient(int index)
	{
		return _koeffizient;
	}
	
	public void setKoeffizient(double value)
	{
		_koeffizient = value;
	}
	
	public String getVariable(double value)
	{
		return _variable;
	}
	
	public String toString()
	{
		return _koeffizient.toString() + _variable;
	}
}
