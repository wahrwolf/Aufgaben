import java.util.ArrayList;


public class LineareGleichung {
	
	//Array mit
	private ArrayList<Object> _koeffizienten;

	public LineareGleichung(ArrayList<Object> _koeffizient) {
		_koeffizienten = _koeffizient;
	}
	

	public Object getKoeffizient(int index)
	{
		return _koeffizienten.get(index);
	}
	

}
